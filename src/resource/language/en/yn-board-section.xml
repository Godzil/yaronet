			<string key="form.field.access">Accessibility*:</string>
			<string key="form.field.access.hint">Defines required permissions to read or write to this section.</string>
			<string key="form.field.access.item.0">Private (requires access to read or write)</string>
			<string key="form.field.access.item.1">Public (read-only, requires write access)</string>
			<string key="form.field.access.item.2">Open (no access required)</string>
			<string key="form.field.description">Description*:</string>
			<string key="form.field.description.hint">Short description of the section.</string>
			<string key="form.field.header">Header:</string>
