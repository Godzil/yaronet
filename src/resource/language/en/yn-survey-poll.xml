<?xml version="1.0" encoding="utf-8"?>
<locale language="en">
	<section prefix="yn.survey.poll.">
		<section prefix="edit.">
			<string key="form.alert">Poll successfully created. Copy the code below to insert it in your message.</string>
			<string key="form.alert.choices-length">Your poll must contain at least two possible answers.</string>
			<string key="form.alert.cost" alias="base.alert.cost" />
			<string key="form.alert.poll-null" alias="base.alert.default" />
